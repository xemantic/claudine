{
  "manifest_version": 3,
  "name": "WebSocket Extension",
  "version": "1.0",
  "description": "Extension with WebSocket connection",
  "permissions": [
    "webRequest",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "ws://localhost:*/*"
  ]
}